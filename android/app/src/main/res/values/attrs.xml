<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextPointView">
        <attr name="text" format="string"/>
        <attr name="redPointViewBgColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CustomizeTextView">
        <attr name="myTextViewFontFamily" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomizeEditText">
        <attr name="myEditTextFontFamily" format="string"/>
    </declare-styleable>
    <declare-styleable name="MultipleRowTextView">
        <attr name="multiRowTextSize" format="dimension"/>
        <attr name="multiRowTextBold" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="VerticalTextView">
        <attr name="verticalTextSize" format="dimension"/>
    </declare-styleable>
</resources>